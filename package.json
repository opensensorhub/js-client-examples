{
  "name": "osh-js",
  "version": "2.0.0",
  "description": "OSH javascript Toolkit",
  "main": "./core/OSH",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensensorhub/osh-js.git"
  },
  "keywords": [
    "OSH",
    "Sensors",
    "Toolkit",
    "Javascript",
    "GIS",
    "Spatial",
    "OGC",
    "SensorML"
  ],
  "author": "OSH community",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/opensensorhub/osh-js/issues"
  },
  "homepage": "https://github.com/opensensorhub/osh-js#readme",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "documentation": "yarn  --cwd jsdoc/ clean && yarn  --cwd jsdoc/ install && yarn  --cwd jsdoc/  all",
    "vuepress": "yarn install && yarn  --cwd vuepress/docs prod",
    "transpile": "shx rm -rf build && shx mkdir -p build && shx cp -rf source/ build && tsc --project config/tsconfig-build.json && cp -fr source/vue ./build/vue",
    "copy-package-resources": "cp -fr source/core/resources ./build/core/resources && cp -fr source/ext/resources ./build/ext/resources",
    "build-package": "npm run transpile && node config/prepare-package && npm run copy-package-resources"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "empty-module": "0.0.2",
    "file-loader": "^5.1.0",
    "jsdoc": "^3.6.4",
    "moment": "^2.24.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "null-loader": "^3.0.0",
    "patch-package": "^6.2.2",
    "strip-ansi": "^6.0.0",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "unminified-webpack-plugin": "^2.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "worker-loader": "^2.0.0",
    "worker-plugin": "^4.0.2",
    "shx": "^0.3.3",
    "wnumb": "^1.2.0",
    "typescript": "^4.0.2",
    "jsdoc-vuejs": "^3.0.9"
  },
  "dependencies": {
  }
}
